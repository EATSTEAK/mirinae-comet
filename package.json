{
  "name": "mirinae-comet",
  "version": "0.0.1",
  "description": "Combined Operation and Management Engaging Tools for SSU Globalmedia student union.",
  "private": true,
  "scripts": {
    "build": "pnpm run --filter=./packages/** --stream build",
    "dev": "pnpm run --filter=./packages/** --stream dev",
    "delete": "pnpm run delete --filter=client && pnpm delete --filter=server --stream",
    "deploy": "pnpm run deploy --filter=server && pnpm deploy --filter=client --stream"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EATSTEAK/mirinae-comet.git"
  },
  "workspaces": [
    "packages/*"
  ],
