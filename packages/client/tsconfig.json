{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "lib": [
      "es5",
      "ES2015.Core",
      "es2015",
      "ES2015.Collection",
      "es2016",
      "ES2016.Array.Include",
      "es2017",
      "es2017.String",
      "es2017.object",
      "es2020",
      "es2019.object",
      "ES2019.Array",
      "DOM",
      "es2015.promise",
      "ES2022.Array",
      "ESNext.Array",
      "esnext"
    ],
    "baseUrl": ".",
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "paths": {
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"]
    }
  },
  "include": [
    "src/**/*.d.ts",
    "src/**/*.js",
    "src/**/*.ts",
    "src/**/*.svelte",
    "../types/index.d.ts"
  ]
}
